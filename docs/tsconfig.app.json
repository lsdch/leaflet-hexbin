{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": [
    "env.d.ts",
    "src/**/*",
    "src/**/*.vue"
  ],
  "exclude": [
    "src/**/__tests__/*"
  ],
  "compilerOptions": {
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "leaflet-hexbin": [
        "../packages/leaflet-hexbin/src/index.ts"
      ],
      "vue-leaflet-hexbin": [
        "../packages/vue-leaflet-hexbin/src/index.ts"
      ],
    },
  }
}