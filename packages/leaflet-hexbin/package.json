{
  "name": "leaflet-hexbin",
  "version": "0.2.0",
  "private": false,
  "type": "module",
  "license": "MIT",
  "description": "Leaflet plugin to create hexbin layers",
  "homepage": "https://lsdch.github.io/leaflet-hexbin/",
  "repository": "git://github.com/lsdch/leaflet-hexbin.git",
  "bugs": {
    "url": "https://github.com/lsdch/leaflet-hexbin/issues"
  },
  "author": {
    "email": "ls.duchemin@gmail.com",
    "name": "Louis Duchemin",
    "url": "https://github.com/lsdch"
  },
  "main": "dist/index.umd.js",
  "module": "dist/index.es.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    }
  },
  "files": [
    "dist/"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "d3": "^7.9.0",
    "d3-hexbin": "^0.2.2",
    "leaflet": "^1.9.4"
  },
  "peerDependencies": {
    "geojson": "^0.5.0"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.1",
    "@types/d3": "^7.4.3",
    "@types/d3-hexbin": "^0.2.5",
    "@types/jsdom": "^21.1.7",
    "@types/leaflet": "^1.9.17",
    "@types/node": "^22.15.18",
    "@vitest/eslint-plugin": "^1.1.44",
    "eslint": "^9.26.0",
    "jiti": "^2.4.2",
    "jsdom": "^26.1.0",
    "npm-run-all2": "^8.0.1",
    "typescript": "~5.8.3",
    "vite-plugin-dts": "^4.5.4"
  }
}